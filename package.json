{
  "name": "discord-login-dashboard",
  "version": "0.1.0",
  "private": true,
  "author": "agustinusnathaniel",
  "engines": {
    "node": ">=22.11.x",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.7.1",
  "pnpm": {
    "neverBuiltDependencies": []
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "turbo": "pnpm dlx turbo@2.5.0",
    "build:turbo": "pnpm turbo run build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "type-check": "tsc --noEmit",
    "check:turbo": "pnpm turbo lint type-check",
    "test:e2e": "playwright test",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 commit-and-tag-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.15.1",
    "@emotion/react": "^11.14.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/js-cookie": "^3.0.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "commit-and-tag-version": "^12.5.0",
    "js-cookie": "^3.0.5",
    "knip": "^5.47.0",
    "next": "14.0.4",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^2.1.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^3.15.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@commitlint/cz-commitlint": "19.8.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@playwright/test": "^1.51.1",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.4.32",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3"
  }
}
